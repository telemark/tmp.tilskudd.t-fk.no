{
  "name": "tmp.tilskudd.t-fk.no",
  "description": "Løsning for søknader om tilskudd",
  "version": "1.0.8",
  "license": "MIT",
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.1.2"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "NODE_ENV=production serve public -p 8000",
    "now-deploy": "now public --static -n tmp-tilskudd && now alias"
  },
  "keywords": [
    "tilskudd",
    "søknadsskjema",
    "microsite",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/tmp.tilskudd.t-fk.no.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/tmp.tilskudd.t-fk.no/issues"
  },
  "homepage": "https://github.com/telemark/tmp.tilskudd.t-fk.no",
  "now": {
    "name": "tmp-tilskudd",
    "alias": [
      "www.tilskudd.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "greenkeeper": {
    "emails": false
  },
  "devDependencies": {
    "coveralls": "2.13.1",
    "nsp": "2.6.3",
    "standard": "10.0.2",
    "tap": "10.7.0"
  },
  "dependencies": {
    "serve": "6.0.0"
  }
}
