{
  "name": "tmp.tilskudd.t-fk.no",
  "description": "Løsning for søknader om tilskudd",
  "version": "1.1.3",
  "license": "MIT",
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "NODE_ENV=production serve public -p 8000",
    "deploy": "now public --static --name tmp-tilskudd && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "tilskudd",
    "søknadsskjema",
    "microsite",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/tmp.tilskudd.t-fk.no.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/tmp.tilskudd.t-fk.no/issues"
  },
  "homepage": "https://github.com/telemark/tmp.tilskudd.t-fk.no#readme",
  "now": {
    "name": "tmp-tilskudd",
    "alias": [
      "www.tilskudd.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "static"
  },
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "devDependencies": {
    "coveralls": "3.0.1",
    "nsp": "3.2.1",
    "standard": "11.0.1",
    "tap": "12.0.1"
  },
  "dependencies": {
    "serve": "6.5.7"
  }
}
